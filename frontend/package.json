{
  "name": "data-brokers-frontend",
  "version": "1.1.0",
  "license": "GPL3",
  "private": true,
  "scripts": {
    "dev": "npm run extract:i18n && npm run compile:i18n && next",
    "start": "next start ./src",
    "build": "next build ./src",
    "deploy": "npx vercel",
    "deploy:prod": "npx vercel --prod",
    "extract:i18n": "formatjs extract 'src/**/*.js' --format crowdin --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format crowdin lang compiled-lang"
  },
  "dependencies": {
    "@changey/react-leaflet-markercluster": "^4.0.0-rc1",
    "@datapunt/matomo-tracker-react": "^0.5.1",
    "@formatjs/cli": "^6.2.12",
    "@formatjs/intl-datetimeformat": "^2.4.3",
    "@formatjs/intl-getcanonicallocales": "^1.3.2",
    "@formatjs/intl-numberformat": "^8.10.3",
    "@formatjs/intl-pluralrules": "^3.4.0",
    "@formatjs/intl-relativetimeformat": "^11.2.14",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "accepts": "^1.3.5",
    "ag-grid-community": "^27.0.0",
    "ag-grid-react": "^27.0.0",
    "cookie-cutter": "^0.2.0",
    "downshift": "^9.0.6",
    "isomorphic-fetch": "^3.0.0",
    "json-to-csv-export": "^2.1.1",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "leaflet.markercluster": "^1.5.3",
    "mailgo": "^0.12.2",
    "mailto-link": "^2.0.0",
    "next": "^14.2.12",
    "next-seo": "^6.6.0",
    "node-gyp": "^10.1.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.8",
    "react-leaflet": "^4.2.1",
    "react-share": "^5.1.0",
    "sitemap": "^6.4.0",
    "universal-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@wdio/cli": "^7.11.0",
    "@wdio/local-runner": "^7.11.0",
    "@wdio/mocha-framework": "^9.0.8",
    "@wdio/spec-reporter": "^7.10.1",
    "babel-plugin-formatjs": "^10.5.16",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chromedriver": "^119.0.1",
    "mailto-parser": "^1.0.1",
    "mocha": "^10.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "3.3.3",
    "wdio-chromedriver-service": "^7.2.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "arrowParens": "avoid"
  }
